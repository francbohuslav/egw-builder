{
    "name": "egw-builder",
    "version": "1.0.0",
    "description": "Scripts for building, testing and release EnergyGateway",
    "main": "index.js",
    "scripts": {
        "start": "node ./index.js -folder ../release_1_1",
        "cleanBuild": "node ./index.js -folder ../release_1_1 -clear -build -metamodel",
        "run": "node ./index.js -folder ../release_1_1 -runDG -runMR -runEMAIL -runFTP",
        "init": "node ./index.js -folder ../release_1_1 -init 12-8835-1",
        "test": "node ./index.js -folder ../release_1_1 -testMR -testFTP -testEMAIL"
    },
    "author": "Bohuslav Franc",
    "license": "ISC",
    "dependencies": {
        "csv-parse": "^4.14.1",
        "prompt-sync": "^4.2.0"
    },
    "devDependencies": {
        "eslint": "^7.15.0",
        "nodemon": "^2.0.6"
    }
}
